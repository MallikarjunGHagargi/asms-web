@import url('https://fonts.googleapis.com/css?family=Raleway:400,500,600,700,800');

/*@import url(https://fonts.googleapis.com/css?family=Source+Sans+Pro:300, 400, 600, 700, 300italic, 400italic, 600italic);*/

html,body{
 width:100%;
 height:100%;
}

a:hover,
a:focus {
    outline: none;
    text-decoration: none;
}

body {
    background: #3c8dbc;
}

.maindiv {
    float: left;
    width: 100%;
}


.divcenter {
    margin: 0 auto;
    width: 50%;
    transition: .8s all ease;
}

.divcenter form#login_form {
    float: left;
    width: 100%;
    background: rgba(14, 121, 158, 0.47);
    padding: 45px 30px 45px;
    width: 100%;
    transition: .8s all ease;
    transform: scaley(0);
    -webkit-transform-origin-y: top;
    transform-origin: top;
}

.divcenter form#login_form:hover {
    background: rgba(14, 121, 158, 0.74);
}

.loginheader {
    float: left;
    font-size: 29px;
    margin: 0 15px 15px;
    color: #fff;
    border-bottom: 2px solid #f1537f;
}

.logininputs {
    float: left;
    width: 100%;
}

.logininputs input {
    height: 50px;
    font-size: 17px;
    margin: 15px 0;
    background: none;
    border: none;
    border-bottom: 1px solid #fff;
    color: #fff;
    transition: .5s all ease;
}

.logininputs label {
    display: none;
}


.loginsubmit {
    float: left;
    width: 100%;
    margin-top: 30px;
}

.loginsubmit button {
    background: #388dc0;
    color: #fff;
    float: right;
    font-size: 15px;
    padding: 11px 28px 9px 17px;
    text-transform: uppercase;
}



/* Form Css */
.sch-reg-frm form{
    float: left;
    width: 100%;
    margin: 10px 0;
}

.red_star {
    color: #ff0000;
}

.sch-reg-frm .form-control:focus {
    background: transparent !important;
    /*    transform: scale(1.03);*/
    transition: 1s all;
    border: none;
    border-bottom-width: medium;
    border-bottom-style: none;
    border-bottom-color: currentcolor;
/*    border-bottom: 2px solid #3c8dbc;*/
border-bottom: 2px solid #fff;
}

.logininputs input:focus {
    border-bottom: 2px solid #fff;
    transition: .5s all ease;
}


::-webkit-input-placeholder {
    color: #fff !important;
    opacity: 0.8!important;
}
:-moz-placeholder {
    /* Firefox 18- */

    color: #fff !important;
    opacity: 0.8!important;
}
::-moz-placeholder {
    /* Firefox 19+ */

    color: #fff !important;
    opacity: 0.8!important;
}
:-ms-input-placeholder {
    /* IE 10+ */

    color: #fff !important;
    opacity: 0.8!important;
}
::-ms-input-placeholder {
    /* Edge */

    color: #fff !important;
    opacity: 0.8!important;
}

.sch-reg-frm label{
    font-weight: 400;
    color: #3c8dbc;
    font-size: 17px;
    margin: 10px 0 0;
}
.sch-reg-frm .errmsg, .sch-reg-frm .errorMsg{
    color: red;
}
.school_registration_form_Submit{
    float: left;
    width: 100%;
    margin-top: -3.8%;
}
.school_registration_form_Submit .btn-default {
    background-color: #367fa9;
    color: #fff;
}
.btn.btn-default.reg_btn_change_color:hover i {
    transform: translateY(-20px);
    transition: 1s all ease-in-out;
    opacity: 0;
}
.school_registration_form_Submit i {
    color: #fff;
}
.sch-reg-frm select{
    color: #adadad !important;
}
.sch-reg-frm .col-sm-6 {
    min-height: 93px;
}
#upload_logo {
    height: auto !important;
}
.content{
    min-height: 200px;
    float: left;
    width: 100%;
}
/*Drag overlay fields*/
textarea#addressline_one {
    z-index: 99999;
    position: absolute;
    width: 92%;
    height: 34px;
}
textarea#addressline_two{
    z-index: 9999;
    position: absolute;
    width: 92%;
    height: 34px;
}
#trust_address {
    z-index: 999999;
    position: absolute;
    width: 92%;
    height: 34px;    
}
/*Error message alignment*/
.errmsgs {
    position: absolute;
    bottom: 0;
    left: 5%;
}

/*Log_in_sec_new*/
body.log_in_sec {
    background: url(../images/Akaperi-Banner-SMS-2.jpg);
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.log_in_sec .maindiv {
    background: rgba(0, 162, 214, 0.83);
    height: 100%;
    display: flex;
    align-items: center;
}

input#loginsubmit {
    background: #f1537f;
    color: #fff;
    width: 100%;
    border: 0;
    padding: 12px 0;
    font-weight: bold;
    font-size: 16px;
}



.abtright {
        float: left;
    width: 100%;
    font-size: 24px;
    line-height: 25px;
    color: #ececec;
        margin-top: -50px;
}

.abtright span {
    font-weight: bold;
    color: #fff;
}

.abtright h3 {
    margin: 0;
    font-size: 44px;
    text-transform: uppercase;
    font-weight: bold;
    color: #fff;
}

.lwrcs {
    text-transform: lowercase !important;
}

.abtright span.lwrcs {
    font-size: 108px;
    margin: 26px 0 0;
}

.abtright h4 {
    font-size: 27px;
}

.abtright span.txt_normal {
    font-size: 49px;
}

 .txt_normal,.txt_rvrse,.abtright h3,.abtright span.lwrcs{
                        display: none;
                    }
                    
                    
                    
                    
                    
/*                    user login*/
input#user_log_sub {
    background: #f1537f;
    color: #fff;
    width: 100%;
    border: 0;
    padding: 12px 0;
    font-weight: bold;
    font-size: 16px;
}


.divcenter form#user_login {
    float: left;
    width: 100%;
    background: rgba(14, 121, 158, 0.47);
    padding: 45px 30px 45px;
    width: 100%;
    transition: .8s all ease;
    transform: scaley(0);
    -webkit-transform-origin-y: top;
    transform-origin: top;
}

.divcenter form#user_login:hover {
    background: rgba(14, 121, 158, 0.74);
}







