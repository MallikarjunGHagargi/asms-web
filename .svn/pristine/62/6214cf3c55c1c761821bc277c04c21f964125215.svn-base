                                        <input class="form-control" name="trust_name" value="" id="trust_name" type="text" onblur="trustname(trust_name)" placeholder="Enter your trust name">
                                        <span class="errmsg"></span>
                                        <div class="errorMsg"></div>
                                    </div>
